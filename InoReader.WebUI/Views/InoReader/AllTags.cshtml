@model List<Domain.Models.TagModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row ">
    <div class="col-sm-12">
        <p class="lead">
            @ViewBag.Title
        </p>
    </div>
</div>
@{
    if (Model.Count != 0)
    {
        foreach (var tag in Model)
        {
            @Html.ActionLink("#"+tag.Title, "GetLinksByTag", "InoReader",
                         new { itemId = tag.TagId, itemName = tag.Title, order = true, page = 0},
                         new { @class = "label label-info" })
            <text>
            &nbsp;
            </text>
        }
    }
    else
    {
        <h1 class="lead">Вы не добавили ни одного тэга</h1>
    }
}